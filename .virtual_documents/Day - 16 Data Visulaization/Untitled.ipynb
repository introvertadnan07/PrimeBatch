


import numpy as np
import pandas as pd
import warnings
warnings.filterwarnings("ignore")
import matplotlib.pyplot as plt
import seaborn as sns


sns.set_theme()


sns.get_dataset_names()


tips = sns.load_dataset("tips")
print(type(tips))


print(tips.head())


sns.set_theme()
sns.scatterplot(
    data=tips,
    x="total_bill",
    y="tip",
    hue="smoker",
    size = "size",
    style="smoker"
)

sns.regplot(
    data=tips,
    x="total_bill",
    y="tip",
    scatter=False,
    color="black"
)

plt.show()



sns.regplot(
    data=tips,
    x="total_bill",
    y="tip",
    kind = "line",
    scatter=False,
    color="black"
)

plt.show()



